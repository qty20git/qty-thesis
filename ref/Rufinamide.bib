@article{ibarra-gonzalez_integrated_2020,
	title = {Integrated {Methodology} for the {Optimal} {Synthesis} of {Lignocellulosic} {Biomass}-to-{Liquid} {Fuel} {Production} {Processes}: 1. {Simulation}-{Based} {Superstructure} {Synthesis} and {Development}},
