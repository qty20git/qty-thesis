	keywords = {Artificial intelligence, Multi-criteria optimization, Process synthesis, Representations, Superstructure optimization, Sustainability, Systematic generation},
	pages = {441--446},
	file = {ScienceDirect Snapshot:D\:\\Program Files (x86)\\Zotero\\Archives\\storage\\QUCBRQFW\\S0098135403002552.html:text/html},
}

