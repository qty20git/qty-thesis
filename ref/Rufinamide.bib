	pages = {14881--14897},
	file = {Full Text PDF:D\:\\Program Files (x86)\\Zotero\\Archives\\storage\\I38S3SZS\\Ibarra-Gonzalez and Rong - 2020 - Integrated Methodology for the Optimal Synthesis o.pdf:application/pdf},
