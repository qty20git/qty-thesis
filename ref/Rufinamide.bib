	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/aic.15370},
	keywords = {algae, mixed integer fractional programming, superstructure optimization, two-stage adaptive robust optimization, uncertainty},
	pages = {582--600},
	file = {Full Text PDF:D\:\\Program Files (x86)\\Zotero\\Archives\\storage\\8PY2AZ6W\\Gong and You - 2017 - Optimal processing network design under uncertaint.pdf:application/pdf;Snapshot:D\:\\Program Files (x86)\\Zotero\\Archives\\storage\\RA22NT3X\\aic.html:text/html},
}

@article{lappas_multi-stage_2016,
